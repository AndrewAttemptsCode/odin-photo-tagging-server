generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id        Int      @id @default(autoincrement())
  username  String
  score     Int
  createdAt DateTime @default(now())
}

model Item {
  id        Int    @id @default(autoincrement())
  name      String
  coordX    Float
  coordY    Float
  deviation Float
}
